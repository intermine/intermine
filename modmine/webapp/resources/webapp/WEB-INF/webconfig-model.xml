  <class className="org.intermine.model.bio.Primer">
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="primaryIdentifier"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.Polypeptide">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
