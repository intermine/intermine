<?xml version="1.0"?>
<classes>
    <class name="org.flymine.model.genomic.ProteinInteractionExperiment" extends="org.flymine.model.genomic.Experiment" is-interface="true">
        <attribute name="name" type="java.lang.String"/>
        <attribute name="hostOrganism" type="java.lang.String"/>
        <reference name="featureDetection" referenced-type="org.flymine.model.genomic.ProteinInteractionTerm"/>
        <reference name="interactionDetection" referenced-type="org.flymine.model.genomic.ProteinInteractionTerm"/>
        <reference name="participantDetection" referenced-type="org.flymine.model.genomic.ProteinInteractionTerm"/>
        <collection name="comments" referenced-type="org.flymine.model.genomic.Comment" ordered="true"/>
        <collection name="interactions" referenced-type="org.flymine.model.genomic.ProteinInteraction" ordered="true" reverse-reference="experiment"/>
    </class>

    <class name="org.flymine.model.genomic.ProteinInteractionTerm" extends="org.flymine.model.genomic.OntologyTerm" is-interface="true"/>

    <class name="org.flymine.model.genomic.ProteinInteraction" is-interface="true">
        <attribute name="shortName" type="java.lang.String"/>
        <reference name="experiment" referenced-type="org.flymine.model.genomic.ProteinInteractionExperiment" reverse-reference="interactions"/>
        <collection name="comments" referenced-type="org.flymine.model.genomic.Comment" ordered="true"/>
        <collection name="interactors" referenced-type="org.flymine.model.genomic.ProteinInteractor" ordered="true" reverse-reference="interaction"/>
        <collection name="interactingRegions" referenced-type="org.flymine.model.genomic.ProteinInteractionRegion" ordered="true" reverse-reference="interaction"/>
        <collection name="proteins" referenced-type="org.flymine.model.genomic.Protein" ordered="true" reverse-reference="proteinInteractions"/>
        <collection name="evidence" referenced-type="org.flymine.model.genomic.Evidence" reverse-reference="relations"/>
    </class>

    <class name="org.flymine.model.genomic.ProteinInteractor" is-interface="true">
        <attribute name="role" type="java.lang.String"/>
        <reference name="protein" referenced-type="org.flymine.model.genomic.Protein" reverse-reference="interactionRoles"/>
        <reference name="interaction" referenced-type="org.flymine.model.genomic.ProteinInteraction" reverse-reference="interactors"/>
    </class>

    <class name="org.flymine.model.genomic.Protein" is-interface="true">
        <collection name="interactionRoles" referenced-type="org.flymine.model.genomic.ProteinInteractor" ordered="true" reverse-reference="protein"/>
        <collection name="interactingProteins" referenced-type="org.flymine.model.genomic.Protein" ordered="true" />
        <collection name="proteinInteractions" referenced-type="org.flymine.model.genomic.ProteinInteraction" ordered="true" reverse-reference="proteins"/>
    </class>

    <class name="org.flymine.model.genomic.Sequence" is-interface="true">
        <attribute name="residues" type="java.lang.String"/>
    </class>

    <class name="org.flymine.model.genomic.AnalysisResult" extends="org.flymine.model.genomic.Evidence" is-interface="true">
        <attribute name="confidenceUnit" type="java.lang.String"/>
        <attribute name="confidenceValue" type="java.lang.String"/>
        <attribute name="confidenceDesc" type="java.lang.String"/>
    </class>

    <class name="org.flymine.model.genomic.ProteinInteractionRegion" is-interface="true">
        <attribute name="name" type="java.lang.String"/>
        <reference name="ontologyTerm" referenced-type="org.flymine.model.genomic.OntologyTerm"/>
        <reference name="protein" referenced-type="org.flymine.model.genomic.Protein"/>
        <reference name="location" referenced-type="org.flymine.model.genomic.Location"/>
        <reference name="interaction" referenced-type="org.flymine.model.genomic.ProteinInteraction" reverse-reference="interactingRegions"/>
    </class>
</classes>
