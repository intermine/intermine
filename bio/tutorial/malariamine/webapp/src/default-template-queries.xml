<userprofile>
  <items/>
  <bags/>
  <queries>
  </queries>
  <template-queries>
    <template name="AAA_organismProteins1" description="Show all the proteins from a particular organism." important="false">
      <query name="AAA_organismProteins1" model="genomic" view="Protein">
        <node path="Protein" type="Protein">
        </node>
        <node path="Protein.organism" type="Organism">
        </node>
        <node path="Protein.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="Show all the proteins from the organism:" identifier="" editable="true" code="A">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAB_Synonyms" description="Show all
                                               names, symbols and identifiers (including synonyms) for a particular
                                               database entry." important="false">
      <query name="AAB_Synonyms" model="genomic" view="Synonym.subject.synonyms.value">
        <node path="Synonym" type="Synonym">
        </node>
        <node path="Synonym.value" type="String">
          <constraint op="=" value="PF11_0344" description="Enter a name or identifier (eg a gene or protein name or identifier):"
                      identifier="" editable="true" code="A"> 
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAB_proteins2fasta" description="Output
                                                     proteins from a particular organism in a format suitable for
                                                     creating a fasta database." important="false">
      <query name="AAB_proteins2fasta" model="genomic" view="Protein">
        <node path="Protein" type="Protein">
        </node>
        <node path="Protein.organism" type="Organism">
        </node>
        <node path="Protein.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="Output proteins from the organism:" identifier="" editable="true" code="A">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAC_organismGenes1" description="Show all the genes for a particular organism." important="false">
      <query name="AAC_organismGenes1" model="genomic" view="Gene">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="Show all the genes from organism:" identifier="" editable="true" code="A">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAC_protein2proteinInteraction" description="Show protein interactions for a particular protein." important="false">
      <query name="AAC_protein2proteinInteraction" model="genomic" view="Protein Protein.interactingProteins Protein.interactingProteins.primaryAccession">
        <node path="Protein" type="Protein">
        </node>
        <node path="Protein.primaryAccession" type="String">
          <constraint op="=" value="P09089" description="Show interactions for protein primary accession:" identifier="" editable="true" code="A">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAD_genes2fasta" description="Output
                                                  genes from a particular organism in a format suitable for creating a
                                                  fasta database." important="false">
      <query name="AAD_genes2fasta" model="genomic" view="Gene Gene.organismDbId Gene.identifier Gene.symbol">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="Output genes from the organism:" identifier="" editable="true" code="A">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAE_gene2Transcripts2" description="Show
                                                        the predicted transcripts, the number of exons and a link to the
                                                        sequence for a particular gene of a particular organism."
              important="false">
      <query name="AAE_gene2Transcripts2" model="genomic" view="Gene Gene.transcripts.identifier Gene.transcripts.exonCount" constraintLogic="A and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.synonyms" type="Synonym">
        </node>
        <node path="Gene.synonyms.value" type="String">
          <constraint op="=" value="PF11_0344" description="Show the predicted
                                                      transcripts, the number of exons and a link to the sequence
                                                      for the following gene:" identifier="" editable="true"
                      code="A"> </constraint>
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="from the following organism:" identifier="" editable="true" code="B">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAF_gene2exonTranscript1" description="Show the predicted transcripts and exons for a particular gene." important="false">
      <query name="AAF_gene2exonTranscript1" model="genomic"
             view="Gene.organismDbId Gene.identifier Gene.name
                   Gene.transcripts.identifier Gene.transcripts.exonCount
                   Gene.exons.identifier" constraintLogic="A and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.synonyms" type="Synonym">
        </node>
        <node path="Gene.synonyms.value" type="String">
          <constraint op="=" value="zen" description="Show predicted transcripts and exons for gene:" identifier="" editable="true" code="A">
          </constraint>
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="From:" identifier="" editable="true" code="B">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAG_gene2protein" description="Show the proteins for a particular gene." important="false">
      <query name="AAG_gene2protein" model="genomic" view="Gene
                                                           Gene.organismDbId Gene.identifier Gene.name Gene.proteins
                                                           Gene.proteins.primaryAccession" constraintLogic="A and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.synonyms" type="Synonym">
        </node>
        <node path="Gene.synonyms.value" type="String">
          <constraint op="=" value="PF11_0344" description="Show proteins for the gene:" identifier="" editable="true" code="A">
          </constraint>
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="From:" identifier="" editable="true" code="B">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAH_Genes2chromosome1" description="Show genes located on a particular chromosome." important="false">
      <query name="AAH_Genes2chromosome1" model="genomic" view="Gene.name Gene.organismDbId Gene.chromosome.identifier" constraintLogic="A and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.chromosome" type="Chromosome">
        </node>
        <node path="Gene.chromosome.identifier" type="String">
          <constraint op="=" value="2L" description="Show genes located on chromosome" identifier="c1" editable="true" code="A">
          </constraint>
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="from" identifier="c2" editable="true" code="B">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAI_gene2ChromosomeLocation1" description="Show the chromosome and the chromosome location of a particular gene." important="false">
      <query name="AAI_gene2ChromosomeLocation1" model="genomic"
             view="Gene Gene.organismDbId Gene.identifier Gene.name
                   Gene.chromosome.identifier Gene.objects.start Gene.objects.end
                   Gene.objects.strand" constraintLogic="A and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.synonyms" type="Synonym">
        </node>
        <node path="Gene.synonyms.value" type="String">
          <constraint op="=" value="PF11_0344" description="Show the chromosome and the chromosome location for gene:" identifier="" editable="true" code="A">
          </constraint>
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="From:" identifier="" editable="true" code="B">
          </constraint>
        </node>
        <node path="Gene.objects" type="Location">
        </node>
        <node path="Gene.objects.object" type="Chromosome">
        </node>
      </query>
    </template>
    <template name="AAK_chromosomeLocation2geneSequence1"
              description="Show the chromosome location and sequence for all genes
                           from a particular organism." important="false">
      <query name="AAK_chromosomeLocation2geneSequence1" model="genomic"
             view="Gene.objects.object.identifier Gene.objects.strand
                   Gene.objects.start Gene.objects.end Gene.organismDbId
                   Gene.identifier Gene.symbol Gene.sequence.residues">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.objects" type="Location">
        </node>
        <node path="Gene.objects.object" type="Chromosome">
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7"
                      description="Show the chromosome location and gene sequences
                                   for all genes from organism:" identifier="" editable="true"
                      code="A"> </constraint>
        </node>
      </query>
    </template>
    <template name="AAL_chromosomeLocation2genes2"
              description="Show the genes, transcripts and exons located between
                           two points on a chromosome.  Give the location of the exons."
              important="false">
      <query name="AAL_chromosomeLocation2genes2" model="genomic"
             view="Chromosome.identifier Chromosome.exons.gene.organismDbId
                   Chromosome.exons.gene.symbol Chromosome.exons.gene.identifier
                   Chromosome.exons.transcripts.identifier
                   Chromosome.exons.identifier Chromosome.exons.objects.start
                   Chromosome.exons.objects.end Chromosome.exons.objects.strand"
             constraintLogic="D and A and C and B">
        <node path="Chromosome" type="Chromosome">
        </node>
        <node path="Chromosome.identifier" type="String">
          <constraint op="=" value="2L" description="Show the genes, transcripts and exons located on chromosome:" identifier="" editable="true" code="A">
          </constraint>
        </node>
        <node path="Chromosome.organism" type="Organism">
        </node>
        <node path="Chromosome.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="from:" identifier="" editable="true" code="B">
          </constraint>
        </node>
        <node path="Chromosome.exons" type="Exon">
        </node>
        <node path="Chromosome.exons.objects" type="Location">
        </node>
        <node path="Chromosome.exons.objects.object" type="Chromosome">
        </node>
        <node path="Chromosome.exons.objects.start" type="Integer">
          <constraint op="&gt;" value="1" description="between base:" identifier="" editable="true" code="C">
          </constraint>
        </node>
        <node path="Chromosome.exons.objects.end" type="Integer">
          <constraint op="&lt;" value="10000" description="and:" identifier="" editable="true" code="D">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAM_Gene2GO2NEW" description="Search for GO annotations for a particular gene." important="false">
      <query name="AAM_Gene2GO2NEW" model="genomic"
             view="Gene.organismDbId Gene.identifier Gene.symbol
                   Gene.goAnnotation.name Gene.goAnnotation.identifier"
             constraintLogic="A and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.synonyms" type="Synonym">
        </node>
        <node path="Gene.synonyms.value" type="String">
          <constraint op="=" value="PF11_0344" description="Show the GO annotations for gene:" identifier="" editable="true" code="A">
          </constraint>
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="from:" identifier="" editable="true" code="B">
          </constraint>
        </node>
      </query>
    </template>
    <template name="AAN_Gene_GoTerms" description="Search for genes annotated with a particular GO term (or any of its children)." important="false">
      <query name="AAN_Gene_GoTerms" model="genomic" view="Gene
                                                           Gene.allGoAnnotation.actualGoTerm.identifier
                                                           Gene.allGoAnnotation.actualGoTerm.name" constraintLogic="A and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="Search for genes from:" identifier="" editable="true" code="A">
          </constraint>
        </node>
        <node path="Gene.allGoAnnotation" type="GOAnnotation">
        </node>
        <node path="Gene.allGoAnnotation.property" type="GOTerm">
        </node>
        <node path="Gene.allGoAnnotation.property.identifier" type="String">
          <constraint op="=" value="GO:0007568" description="that have
                                                             been annotated with GO term (or any of its children):"
                      identifier="GOTerm.identifier" editable="true" code="B">
          </constraint>
        </node>
      </query>
    </template>
    <template name="GOterm2GeneProteinInteraction" description="Show
                                                                genes from a particular organism annotated with a particular GO term
                                                                which has evidence code IPI (inferred from Protein
                                                                Interaction). Show the gene from which the annotation was inferred
                                                                (the 'with' field)." important="false">
      <query name="GOterm2GeneProteinInteraction" model="genomic" view="Gene Gene.goAnnotation Gene.goAnnotation.with" constraintLogic="A and C and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="Show genes from organism:" identifier="" editable="true" code="A">
          </constraint>
        </node>
        <node path="Gene.goAnnotation" type="GOAnnotation">
        </node>
        <node path="Gene.goAnnotation.name" type="String">
          <constraint op="=" value="protein binding" description="annotated with GO term (and evidence code IPI):" identifier="" editable="true" code="B">
          </constraint>
        </node>
        <node path="Gene.goAnnotation.evidenceCode" type="String">
          <constraint op="=" value="IPI" description="" identifier="" code="C">
          </constraint>
        </node>
      </query>
    </template>
    <template name="InteractionDataSets" description="Show all protein interactions from a particular dataset." important="false">
      <query name="InteractionDataSets" model="genomic" view="ProteinInteraction">
        <node path="ProteinInteraction" type="ProteinInteraction">
        </node>
        <node path="ProteinInteraction.evidence" type="ExperimentalResult">
        </node>
        <node path="ProteinInteraction.evidence.analysis" type="ProteinInteractionExperiment">
        </node>
        <node path="ProteinInteraction.evidence.analysis.publication" type="Publication">
        </node>
        <node path="ProteinInteraction.evidence.analysis.publication.firstAuthor" type="String">
          <constraint op="LIKE" value="Giot%" description="Show all
                                                           protein interactions from the dataset (leave as default or
                                                           change to Stanyon* or Li*):" identifier="" editable="true"
                      code="A"> </constraint>
        </node>
      </query>
    </template>
    <template name="Object_Synonym" description="Find an entry in the database using any identifier or synonym." important="false">
      <query name="Object_Synonym" model="genomic" view="Synonym.subject">
        <node path="Synonym" type="Synonym">
        </node>
        <node path="Synonym.value" type="String">
          <constraint op="=" value="timeout" description="" identifier="Synonym.value.1" editable="true" code="A">
          </constraint>
        </node>
      </query>
    </template>
    <template name="gene2goTermwithEvidence"
              description="Show genes from a particular organism annotated with a
                           particular GO term with a particular evidence code."
              important="false">
      <query name="gene2goTermwithEvidence" model="genomic" view="Gene Gene.goAnnotation" constraintLogic="A and C and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7" description="Show genes from organism:" identifier="" editable="true" code="A">
          </constraint>
        </node>
        <node path="Gene.goAnnotation" type="GOAnnotation">
        </node>
        <node path="Gene.goAnnotation.name" type="String">
          <constraint op="=" value="protein binding" description="annotated with GO term:" identifier="" editable="true" code="B">
          </constraint>
        </node>
        <node path="Gene.goAnnotation.evidenceCode" type="String">
          <constraint op="=" value="IPI" description="with evidence code:" identifier="" editable="true" code="C">
          </constraint>
        </node>
      </query>
    </template>
    <template name="geneTranscriptsExons" description="For a
                                                       particular gene, list the transcripts and show the chromosome
                                                       location and length of their exons." important="false">
      <query name="geneTranscriptsExons" model="genomic"
             view="Gene.identifier Gene.transcripts.identifier
                   Gene.transcripts.exons.identifier
                   Gene.transcripts.exons.chromosomeLocation.start
                   Gene.transcripts.exons.chromosomeLocation.end
                   Gene.transcripts.exons.length">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.identifier" type="String">
          <constraint op="=" value="CG10016" description="Show transcripts and exons for gene:" identifier="Gene.identifier" editable="true" code="A">
          </constraint>
        </node>
      </query>
    </template>
    <template name="geneTranscriptsIntrons" description="For
                                                         a particular gene, list the transcripts and show the chromosome
                                                         location and length of their introns." important="false">
      <query name="geneTranscriptsIntrons" model="genomic"
             view="Gene.identifier Gene.transcripts.identifier
                   Gene.transcripts.introns
                   Gene.transcripts.introns.chromosomeLocation.start
                   Gene.transcripts.introns.chromosomeLocation.end
                   Gene.transcripts.introns.length">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.identifier" type="String">
          <constraint op="=" value="CG10016" description="Show transcripts and introns for gene:" identifier="Gene.identifier" editable="true" code="A">
          </constraint>
        </node>
      </query>
    </template>
    <template name="goTerm2genesWithEvidenceandWith"
              description="Show genes and the related 'with' annotations from a
                           particular organism annotated with a particular GO term with a
                           particular evidence code. (The 'with' field holds an additional
                           identifier for annotations using certain evidence codes - see
                           below)." important="false">
      <query name="goTerm2genesWithEvidenceandWith" model="genomic" view="Gene Gene.goAnnotation Gene.goAnnotation.with" constraintLogic="A and C and B">
        <node path="Gene" type="Gene">
        </node>
        <node path="Gene.organism" type="Organism">
        </node>
        <node path="Gene.organism.name" type="String">
          <constraint op="=" value="Plasmodium falciparum 3D7"
                      description="Show genes and 'with' annotations from organism:"
                      identifier="" editable="true" code="A"> </constraint>
        </node>
        <node path="Gene.goAnnotation" type="GOAnnotation">
        </node>
        <node path="Gene.goAnnotation.name" type="String">
          <constraint op="=" value="protein binding" description="Annotated with GO term:" identifier="" editable="true" code="B">
          </constraint>
        </node>
        <node path="Gene.goAnnotation.evidenceCode" type="String">
          <constraint op="=" value="IPI" description="with evidence
                                                      code: (note: for 'with' annotations the evidence code should
                                                      be IC, IEA, IGI, IPI OR ISS)" identifier="" editable="true"
                      code="C"> </constraint>
        </node>
      </query>
    </template>
    <template name="proteinToGeneIdentifiers"
              description="For a particular UniProt protein show the identifiers
                           and symbol of the corresponding gene." important="false">
      <query name="proteinToGeneIdentifiers" model="genomic"
             view="Protein.identifier Protein.primaryAccession Protein.name
                   Protein.genes.identifier Protein.genes.symbol
                   Protein.genes.organismDbId">
        <node path="Protein" type="Protein">
        </node>
        <node path="Protein.synonyms" type="Synonym">
        </node>
        <node path="Protein.synonyms.value" type="String">
          <constraint op="=" value="128UP_DROME" description="Enter the
                                                              identifier or accession of a UniProt protein:" identifier=""
                      editable="true" code="A"> </constraint>
        </node>
      </query>
    </template>
  </template-queries>

