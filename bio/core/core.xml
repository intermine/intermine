<?xml version="1.0"?>
<model name="genomic" namespace="http://www.flymine.org/model/genomic#">
  <class name="org.flymine.model.genomic.DataSource" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <attribute name="url" type="java.lang.String"/>
    <attribute name="description" type="java.lang.String"/>
  </class>
  <class name="org.flymine.model.genomic.DataSet" extends="org.flymine.model.genomic.InfoSource" is-interface="true">
    <attribute name="url" type="java.lang.String"/>
    <attribute name="description" type="java.lang.String"/>
    <attribute name="version" type="java.lang.String"/>
    <reference name="dataSource" referenced-type="org.flymine.model.genomic.DataSource"/>
  </class>
  <class name="org.flymine.model.genomic.Ontology" extends="org.flymine.model.genomic.InfoSource" is-interface="true">
    <attribute name="url" type="java.lang.String"/>
  </class>
  <class name="org.flymine.model.genomic.BioEntity" is-interface="true">
    <attribute name="identifier" type="java.lang.String"/>
    <attribute name="name" type="java.lang.String"/>
    <attribute name="curated" type="java.lang.Boolean"/>
    <reference name="organism" referenced-type="org.flymine.model.genomic.Organism"/>
    <collection name="relations" referenced-type="org.flymine.model.genomic.SymmetricalRelation" ordered="true" reverse-reference="bioEntities"/>
    <collection name="subjects" referenced-type="org.flymine.model.genomic.Relation" ordered="true" reverse-reference="object"/>
    <collection name="objects" referenced-type="org.flymine.model.genomic.Relation" ordered="true" reverse-reference="subject"/>
    <collection name="annotations" referenced-type="org.flymine.model.genomic.Annotation" ordered="true" reverse-reference="subject"/>
    <collection name="synonyms" referenced-type="org.flymine.model.genomic.Synonym" ordered="true" reverse-reference="subject"/>
    <collection name="evidence" referenced-type="org.flymine.model.genomic.Evidence" ordered="true"/>
  </class>
  <class name="org.flymine.model.genomic.ComputationalAnalysis" extends="org.flymine.model.genomic.Analysis" is-interface="true">
    <attribute name="algorithm" type="java.lang.String"/>
    <attribute name="description" type="java.lang.String"/>
  </class>
  <class name="org.flymine.model.genomic.Analysis" is-interface="true">
    <reference name="publication" referenced-type="org.flymine.model.genomic.Publication"/>
  </class>
  <class name="org.flymine.model.genomic.Relation" extends="org.flymine.model.genomic.SymmetricalRelation" is-interface="true">
    <reference name="subject" referenced-type="org.flymine.model.genomic.BioEntity" reverse-reference="objects"/>
    <reference name="object" referenced-type="org.flymine.model.genomic.BioEntity" reverse-reference="subjects"/>
    <collection name="evidence" referenced-type="org.flymine.model.genomic.Evidence" ordered="true" reverse-reference="relations"/>
  </class>
  <class name="org.flymine.model.genomic.Experiment" extends="org.flymine.model.genomic.Analysis" is-interface="true"/>
  <class name="org.flymine.model.genomic.Location" extends="org.flymine.model.genomic.Relation" is-interface="true">
    <attribute name="end" type="java.lang.Integer"/>
    <attribute name="endPhase" type="java.lang.Integer"/>
    <attribute name="phase" type="java.lang.Integer"/>
    <attribute name="start" type="java.lang.Integer"/>
    <attribute name="endIsPartial" type="java.lang.Boolean"/>
    <attribute name="strand" type="java.lang.String"/>
    <attribute name="startIsPartial" type="java.lang.Boolean"/>
  </class>
  <class name="org.flymine.model.genomic.Evidence" is-interface="true">
    <collection name="relations" referenced-type="org.flymine.model.genomic.Relation" ordered="true" reverse-reference="evidence"/>
  </class>
  <class name="org.flymine.model.genomic.ComputationalResult" extends="org.flymine.model.genomic.AnalysisResult" is-interface="true">
    <attribute name="score" type="java.lang.Double"/>
    <attribute name="type" type="java.lang.String"/>
  </class>
  <class name="org.flymine.model.genomic.OntologyRelation" is-interface="true">
    <attribute name="type" type="java.lang.String"/>
    <reference name="parentTerm" referenced-type="org.flymine.model.genomic.OntologyTerm" reverse-reference="childRelations"/>
    <reference name="childTerm" referenced-type="org.flymine.model.genomic.OntologyTerm" reverse-reference="parentRelations"/>
  </class>
  
  <class name="org.flymine.model.genomic.SymmetricalRelation" is-interface="true">
    <collection name="bioEntities" referenced-type="org.flymine.model.genomic.BioEntity" ordered="true" reverse-reference="relations"/>
  </class>
  <class name="org.flymine.model.genomic.OverlapRelation" extends="org.flymine.model.genomic.SymmetricalRelation" is-interface="true">
  </class>
  <class name="org.flymine.model.genomic.Publication" extends="org.flymine.model.genomic.InfoSource" is-interface="true">
    <attribute name="issue" type="java.lang.String"/>
    <attribute name="pubMedId" type="java.lang.String"/>
    <attribute name="pages" type="java.lang.String"/>
    <attribute name="year" type="java.lang.Integer"/>
    <attribute name="volume" type="java.lang.String"/>
    <attribute name="journal" type="java.lang.String"/>
    <attribute name="firstAuthor" type="java.lang.String"/>
    <collection name="authors" referenced-type="org.flymine.model.genomic.Author" reverse-reference="publications" ordered="true"/>
  </class>
  <class name="org.flymine.model.genomic.InfoSource" extends="org.flymine.model.genomic.Evidence" is-interface="true">
    <attribute name="title" type="java.lang.String"/>
  </class>
  <class name="org.flymine.model.genomic.Annotation" is-interface="true">
    <attribute name="qualifier" type="java.lang.String"/>
    <reference name="property" referenced-type="org.flymine.model.genomic.BioProperty" reverse-reference="annotations"/>
    <reference name="subject" referenced-type="org.flymine.model.genomic.BioEntity" reverse-reference="annotations"/>
    <collection name="evidence" referenced-type="org.flymine.model.genomic.Evidence" ordered="true"/>
  </class>
  <class name="org.flymine.model.genomic.BioProperty" is-interface="true">
    <collection name="annotations" referenced-type="org.flymine.model.genomic.Annotation" ordered="true" reverse-reference="property"/>
    <collection name="evidence" referenced-type="org.flymine.model.genomic.Evidence" ordered="true"/>
  </class>
  <class name="org.flymine.model.genomic.SimpleRelation" extends="org.flymine.model.genomic.Relation" is-interface="true"/>
  <class name="org.flymine.model.genomic.OntologyTerm" extends="org.flymine.model.genomic.BioProperty" is-interface="true">
    <attribute name="identifier" type="java.lang.String"/>
    <attribute name="name" type="java.lang.String"/>
    <attribute name="description" type="java.lang.String"/>
    <attribute name="namespace" type="java.lang.String"/>
    <attribute name="obsolete" type="java.lang.Boolean"/>
    <attribute name="depth" type="java.lang.Integer"/>
    <reference name="ontology" referenced-type="org.flymine.model.genomic.Ontology"/>
    <collection name="parentRelations" referenced-type="org.flymine.model.genomic.OntologyRelation" ordered="true" reverse-reference="childTerm"/>
    <collection name="childRelations" referenced-type="org.flymine.model.genomic.OntologyRelation" ordered="true" reverse-reference="parentTerm"/>
    <collection name="synonyms" referenced-type="org.flymine.model.genomic.OntologyTermSynonym" ordered="true"/>
  </class>
  <class name="org.flymine.model.genomic.OntologyTermSynonym" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <attribute name="type" type="java.lang.String"/>
  </class>
  <class name="org.flymine.model.genomic.Synonym" is-interface="true">
    <attribute name="value" type="java.lang.String"/>
    <attribute name="type" type="java.lang.String"/>
    <attribute name="isPrimary" type="java.lang.Boolean"/>
    <reference name="subject" referenced-type="org.flymine.model.genomic.BioEntity" reverse-reference="synonyms"/>
    <reference name="source" referenced-type="org.flymine.model.genomic.DataSource"/>
    <collection name="evidence" referenced-type="org.flymine.model.genomic.InfoSource"/>
  </class>
  <class name="org.flymine.model.genomic.Author" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <collection name="publications" referenced-type="org.flymine.model.genomic.Publication" reverse-reference="authors" ordered="true"/>
  </class>
  <class name="org.flymine.model.genomic.ExperimentalResult" extends="org.flymine.model.genomic.AnalysisResult" is-interface="true"/>
  <class name="org.flymine.model.genomic.PartialLocation" extends="org.flymine.model.genomic.Location" is-interface="true">
    <attribute name="subjectStart" type="java.lang.Integer"/>
    <attribute name="subjectEnd" type="java.lang.Integer"/>
  </class>
  <class name="org.flymine.model.genomic.Organism" is-interface="true">
    <attribute name="genus" type="java.lang.String"/>
    <attribute name="taxonId" type="java.lang.Integer"/>
    <attribute name="species" type="java.lang.String"/>
    <attribute name="abbreviation" type="java.lang.String"/>
    <attribute name="shortName" type="java.lang.String"/>
    <attribute name="name" type="java.lang.String"/>
  </class>
  <class name="org.flymine.model.genomic.RankedRelation" extends="org.flymine.model.genomic.Relation" is-interface="true">
    <attribute name="rank" type="java.lang.Integer"/>
  </class>
  <class name="org.flymine.model.genomic.AnalysisResult" extends="org.flymine.model.genomic.Evidence" is-interface="true">
    <attribute name="confidence" type="java.lang.Float"/>
    <reference name="analysis" referenced-type="org.flymine.model.genomic.Analysis"/>
    <reference name="source" referenced-type="org.flymine.model.genomic.InfoSource"/>
  </class>
  <class name="org.flymine.model.genomic.Comment" is-interface="true">
    <attribute name="text" type="java.lang.String"/>
    <attribute name="type" type="java.lang.String"/>
    <reference name="source" referenced-type="org.flymine.model.genomic.InfoSource"/>
  </class>
  <class name="org.flymine.model.genomic.Gene" is-interface="true">
    <attribute name="organismDbId" type="java.lang.String"/>
    <attribute name="symbol" type="java.lang.String"/>
    <attribute name="name" type="java.lang.String"/>
    <collection name="proteins" referenced-type="org.flymine.model.genomic.Protein" ordered="true" reverse-reference="genes"/>
  </class>
  <class name="org.flymine.model.genomic.Protein" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <attribute name="primaryAccession" type="java.lang.String"/>
    <attribute name="length" type="java.lang.Integer"/>
    <attribute name="molecularWeight" type="java.lang.Integer"/>
    <reference name="sequence" referenced-type="org.flymine.model.genomic.Sequence"/>
    <collection name="genes" referenced-type="org.flymine.model.genomic.Gene" ordered="true" reverse-reference="proteins"/>
  </class>
  <class name="org.flymine.model.genomic.Sequence" is-interface="true">
    <attribute name="residues" type="java.lang.String"/>
    <attribute name="length" type="int"/>
  </class>
</model>
