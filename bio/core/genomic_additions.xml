<?xml version="1.0"?>
<classes>

    <class name="org.flymine.model.genomic.CDS" is-interface="true">
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene"
                   reverse-reference="CDSs"/>
        <reference name="protein" referenced-type="org.flymine.model.genomic.Protein"/>
        <reference name="translation" referenced-type="org.flymine.model.genomic.Translation"/>
    </class>

    <class name="org.flymine.model.genomic.Exon" is-interface="true">
        <collection name="transcripts" referenced-type="org.flymine.model.genomic.Transcript"
                    reverse-reference="exons" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.Enhancer" is-interface="true">
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene"/>
    </class>

    <class name="org.flymine.model.genomic.FivePrimeUTR" is-interface="true">
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene"/>
    </class>

    <class name="org.flymine.model.genomic.Gene" is-interface="true">
       <attribute name="ncbiGeneNumber" type="java.lang.String"/>
       <collection name="UTRs" referenced-type="org.flymine.model.genomic.UTR"
                    reverse-reference="gene" ordered="true"/>
        <collection name="CDSs" referenced-type="org.flymine.model.genomic.CDS"
                    reverse-reference="gene" ordered="true"/>
        <collection name="publications" referenced-type="org.flymine.model.genomic.Publication" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.Intron" is-interface="true">
        <collection name="transcripts" referenced-type="org.flymine.model.genomic.Transcript"
                    reverse-reference="introns" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.UTR" is-interface="true">
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene"
                   reverse-reference="UTRs"/>
        <collection name="MRNAs" referenced-type="org.flymine.model.genomic.MRNA"
                    reverse-reference="UTRs" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.Transcript" is-interface="true">
        <attribute name="organismDbId" type="java.lang.String"/>
        <attribute name="exonCount" type="java.lang.Integer"/>
        <reference name="protein" referenced-type="org.flymine.model.genomic.Protein"/>
        <collection name="introns" referenced-type="org.flymine.model.genomic.Intron" ordered="true"
                    reverse-reference="transcripts"/>
        <collection name="exons" referenced-type="org.flymine.model.genomic.Exon"
                    reverse-reference="transcripts" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.NcRNA" is-interface="true"/>

    <class name="org.flymine.model.genomic.Protein" is-interface="true">
        <collection name="translations" referenced-type="org.flymine.model.genomic.Translation"
                    reverse-reference="protein" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.Translation" is-interface="true">
        <attribute name="symbol" type="java.lang.String"/>
        <attribute name="organismDbId" type="java.lang.String"/>
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene"/>
        <reference name="sequence" referenced-type="org.flymine.model.genomic.Sequence"/>
        <reference name="protein" referenced-type="org.flymine.model.genomic.Protein"
                   reverse-reference="translations"/>
        <reference name="MRNA" referenced-type="org.flymine.model.genomic.MRNA" reverse-reference="translation"/>
        <collection name="CDSs" referenced-type="org.flymine.model.genomic.CDS"
                    reverse-reference="translation" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.ProteinBindingSite" is-interface="true"/>

    <class name="org.flymine.model.genomic.Pseudogene" is-interface="true"/>

    <class name="org.flymine.model.genomic.TransposableElementInsertionSite" is-interface="true"/>

    <class name="org.flymine.model.genomic.InsertionSite" is-interface="true">
        <collection name="genes" referenced-type="org.flymine.model.genomic.Gene" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.MRNA" is-interface="true">
        <reference name="fivePrimeUTR" referenced-type="org.flymine.model.genomic.FivePrimeUTR"/>
        <reference name="threePrimeUTR" referenced-type="org.flymine.model.genomic.ThreePrimeUTR"/>
        <reference name="translation" referenced-type="org.flymine.model.genomic.Translation" reverse-reference="MRNA"/>
        <collection name="UTRs" referenced-type="org.flymine.model.genomic.UTR"
                    reverse-reference="MRNAs" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.SequenceVariant" is-interface="true">
        <collection name="genes" referenced-type="org.flymine.model.genomic.Gene" ordered="true"/>
    </class>

    <class name="org.flymine.model.genomic.TransposableElement" is-interface="true">
        <attribute name="organismDbId" type="java.lang.String"/>
    </class>

    <class name="org.flymine.model.genomic.ThreePrimeUTR" is-interface="true">
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene"/>
    </class>

</classes>
